<table class="table table-bordered">
    <thead>
        <tr>
            <th>Product</th>
            <th>Name</th>
            <th>Quantity</th>
            <th>Price(VND)</th>
            <th>Price(USD)</th>
            <th>Total</th>
        </tr>
    </thead>
    <tbody>
        <% let totalPriceVND = 0,totalPriceUSD = 0; %> 
        <% cartInfo.forEach((product)=>{ %>
            <tr>
                <td> <img width="60" src="<%= product.thumbnail_url %>" alt="<%= product.product_name %>" /></td>
                <td>
                    <%= product.product_name %>
                </td>
                <td>
                    <div class="input-append">
                        <input class="span1" style="max-width:34px" value="<%= product.quantity %>" id="appendedInputButtons" size="16" type="text">
                            <button class="btn" type="button"><i class="icon-minus"></i></button>
                            <button class="btn" type="button"><i class="icon-plus"></i></button>
                            <button class="btn btn-danger" type="button"><i class="icon-remove icon-white"></i></button>
                    </div>
                </td>
                <td>
                    <%=  moneyConvert(String(product.price)) %>
                </td>
                <td>
                    $<%= product.price_usd %>
                </td>
                <td>$110.00</td>
            </tr>
            <% totalPriceVND += product.price*product.quantity; totalPriceUSD += product.price_usd*product.quantity; %>
            <% }) %>

                <tr>
                    <td colspan="5" style="text-align:right">Total Price(VND): </td>
                    <td class="label label-important" style="display:block"> <%= moneyConvert(String(totalPriceVND))  %></td>
                </tr>
                <tr>
                    <td colspan="5" style="text-align:right" >Total Price(USD): </td>
                    <td class="label label-important" style="display:block"> $<%= moneyConvert(String(totalPriceUSD.toFixed(0)))  %></td>
                </tr>
    </tbody>
</table>