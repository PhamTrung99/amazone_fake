<ul class="thumbnails">
  <% _10Pro.forEach(pro => { %>
    <li class="span3">
      <div class="thumbnail">
        <a href="product_details.html"><img src="<%= pro.thumbnail_url %>" alt="<%= pro.name %>" /></a>
        <div class="caption">
          <h6><%= pro.name.substring(0,70) + '...' %> </h6>

          <h4 style="text-align:center">
            <a class="btn" href="product_details.html" style="background: FIREBRICK;">
              <i style="color: white ;font-style: inherit" >-<%= pro.discount_rate %>%</i>
            </a>
            <a class="btn" href="#">Add to <i class="icon-shopping-cart"></i></a>
            <a class="btn btn-primary" href="#"><%= pro.list_price %> Ä‘</a>
          </h4>
        </div>
      </div>
    </li>
   <% }); %>
</ul>
