<body>
	<%- include('../layouts/header.ejs') %>
		<!-- Header End====================================================================== -->
		<div id="mainBody">
			<div class="container">
				<div class="row">
					<!-- Sidebar ================================================== -->
					<%- include('../layouts/sideBar.ejs') %>
						<!-- Sidebar end=============================================== -->
						<div class="span9">
							<ul class="breadcrumb">
								<li>Home <span class="divider">/</span></li>
								<li>Products <span class="divider">/</span></li>
								<li class="active"><a href="/">
										<%= proInfo.name %>
									</a></li>
							</ul>
							<div class="row">
								<div id="gallery" class="span3">
									<a href="<%= proInfo.thumbnail_url %>" title="<%= proInfo.name %>">
										<img src="<%= proInfo.thumbnail_url %>" style="width:100%"
											alt="<%= proInfo.name %>" />
									</a>
									<div id="differentview" class="moreOptopm carousel slide">
										<div class="carousel-inner">
											<div class="item active">
												<a href="<%= proInfo.thumbnail_url %>"> <img style="width:29%"
														src="<%= proInfo.thumbnail_url %>" alt="" /></a>
											</div>
											<div class="item">
												<a href="<%= proInfo.thumbnail_url %>"> <img style="width:29%"
														src="<%= proInfo.thumbnail_url %>" alt="" /></a>
											</div>
										</div>
										<!--  
			  <a class="left carousel-control" href="#myCarousel" data-slide="prev">‹</a>
              <a class="right carousel-control" href="#myCarousel" data-slide="next">›</a> 
			  -->
									</div>

									<div class="btn-toolbar">
										<div class="btn-group">
											<span class="btn"><i class="icon-envelope"></i></span>
											<span class="btn"><i class="icon-print"></i></span>
											<span class="btn"><i class="icon-zoom-in"></i></span>
											<span class="btn"><i class="icon-star"></i></span>
											<span class="btn"><i class=" icon-thumbs-up"></i></span>
											<span class="btn"><i class="icon-thumbs-down"></i></span>
										</div>
									</div>
								</div>
								<div class="span6">
									<h3>
										<%= proInfo.name %>
									</h3>
									<hr class="soft" />
									<!-- <form class="form-horizontal qtyFrm">
				  <div class="control-group">
					<label class="control-label"><span style="font-size: 25px;font-family: fantasy"><%= proInfo.price %> VNĐ</span>
						<del><%= proInfo.list_price %>₫ </del>
						<button style=" background-color: red;border: none;color: white;font-weight: 900;padding: 3px;/* text-align: center; *//* text-decoration: none; */display: inline-block;font-size: 10px;margin: 4px 2px;border-radius: 50%;">-<%= proInfo.discount_rate %>%</button></label>
					<div class="controls">
					
					</div>
				  </div>
				</form> -->
									<div class="row" style="padding-left: 28px;">
										<label class="control-label"><span style="font-size: 25px;font-family: fantasy">
												<%= price %> VNĐ
											</span>
											<del style="margin-left: 9px;">
												<%= proInfo.list_price %>₫
											</del>
											<button
												style=" background-color: red;border: none;color: white;font-weight: 900;padding: 3px;/* text-align: center; *//* text-decoration: none; */display: inline-block;font-size: 10px;margin: 4px 2px;border-radius: 50%;">-
												<%= proInfo.discount_rate %>%
											</button></label>
									</div>
									<div class="row" style="padding-top: 10px;padding-left: 23px;">
										<input type="number" min="0" class="span1" placeholder="Số lượng" />
										<button type="submit" class="btn btn-large btn-danger"
											style="margin-left: 27px"> Add to cart <i
												class=" icon-shopping-cart"></i></button>

									</div>
								</div>

								<div class="span9">
									<div id="myTabContent" class="tab-content">
										<div class="tab-pane fade active in" id="home">
											<h4 style="font-size: 24px;font-family: cursive;margin-top: 30px;margin-bottom: 18px;">Product Information</h4>
											<table class="table">
												<tbody>
													<% proInfo.specifications[0].attributes.forEach((obj)=>{  %> 
													<tr>
														<td style="background-color: #EFEFEF;"><%= obj.name %></td>
														<td><%= obj.value %></td>
													</tr>
													<% }) %> 
												</tbody>
											</table>
											<h4 style="font-size: 24px;font-family: cursive;margin-top: 37px;margin-bottom: 18px;">Description</h4>
											<div style="overflow: auto;height: 35rem;">
												<%- proInfo.description %>
											</div>
										</div>
									</div>
								</div>
						

							</div>
						</div>
				</div>
				<div class="row">
					<h4 style="font-size: 24px;font-family: cursive;margin-top: 30px;margin-bottom: 18px;">COMMENTS</h4>
				</div>
			</div>
		</div>
